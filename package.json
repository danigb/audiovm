{
  "name": "audiovm",
  "description": "Audio Virtual Machine",
  "homepage": "https://github.com/danigb/audiovm#readme",
  "version": "1.0.0",
  "jsnext:main": "src/index.js",
  "module": "src/index.js",
  "repository": "danigb/audiovm",
  "keywords": ["audio", "virtual", "machine", "gibberish"],
  "author": "danigb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danigb/audiovm/issues"
  },
  "dependencies": {
    "microtonal": "^1.0.0"
  },
  "scripts": {
    "test": "jest --coverage --no-cache",
    "build": "rollup -c",
    "prepublish": "npm run build",
    "format": "prettier --semi false --write 'src/**/*.js'",
    "release": "npm run build && npm test && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish",
    "watch": "rollup -c --watch",
    "docs": "jsdoc src/ -r -d docs/jsdoc"
  },
  "devDependencies": {
    "babel-preset-es2015-rollup": "^3.0.0",
    "jest": "^20.0.4",
    "prettier": "^1.5.3",
    "rollup": "^0.47.6",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-filesize": "^1.4.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1"
  },
  "jest": {
    "transformIgnorePatterns": ["<rootDir>/node_modules/(?!microtonal)"]
  }
}
